{
  "name": "finance-tracker",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "db:generate:client": "npx prisma generate",
    "db:migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev",
    "db:migrate:prod": "dotenv -e .env -- npx prisma migrate dev",
    "db:migrate:reset:dev": "dotenv -e .env.development -- npx prisma migrate reset",
    "db:migrate:reset:prod": "dotenv -e .env -- npx prisma migrate reset --skip-seed",
    "dev": "remix vite:dev --host",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "netlify serve",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.4.2",
    "@netlify/functions": "^2.6.0",
    "@netlify/remix-adapter": "^2.3.1",
    "@prisma/client": "^5.14.0",
    "@remix-run/node": "^2.9.2",
    "@remix-run/react": "^2.9.2",
    "@remix-run/serve": "^2.9.2",
    "@supabase/ssr": "^0.3.0",
    "@xstate/react": "^4.1.1",
    "cva": "npm:class-variance-authority@^0.7.0",
    "dotenv-cli": "^7.4.2",
    "isbot": "^4.1.0",
    "lucide-react": "^0.381.0",
    "papaparse": "^5.4.1",
    "react-hook-form": "^7.51.5",
    "remix-auth": "^3.7.0",
    "remix-auth-form": "^1.5.0",
    "xstate": "^5.13.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.5.0",
    "@faker-js/faker": "^8.4.1",
    "@remix-run/dev": "^2.9.2",
    "@storybook/addon-essentials": "^8.1.6",
    "@storybook/addon-interactions": "^8.1.6",
    "@storybook/addon-links": "^8.1.6",
    "@storybook/blocks": "^8.1.6",
    "@storybook/builder-vite": "^8.1.6",
    "@storybook/react": "^8.1.6",
    "@storybook/react-vite": "^8.1.6",
    "@storybook/test": "^8.1.6",
    "@tailwindcss/forms": "^0.5.7",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "jsdom": "^24.1.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "prisma": "^5.14.0",
    "storybook": "^8.1.6",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.11.0",
    "typescript": "^5.1.6",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
